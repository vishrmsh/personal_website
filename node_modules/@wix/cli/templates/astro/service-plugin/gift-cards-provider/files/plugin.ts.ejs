import { giftVouchersProvider } from '@wix/ecom/service-plugins';

giftVouchersProvider.provideHandlers({
  redeem: async ({ request, metadata }) => {
    return {
      remainingBalance: 80.00,
      currencyCode: 'USD',
      transactionId: '00000000-0000-0000-0000-000000000001',
    };
  },
  _void: async ({ request, metadata }) => {
    return {
      remainingBalance: 100.0,
      currencyCode: 'USD',
    };
  },
  getBalance: async ({ request, metadata }) => {
    return {
      balance: 100.0,
      currencyCode: 'USD',
    };
  },
});
